CompileFlags:
  Add:
    - -m32
    - -ffreestanding
    - -fno-pic
    - -fno-pie
    - -nostdlib
    - -I/home/dddddd/newos/os2/include
    - -I./include
    - -Iinclude
  Remove:
    - -stdlib=*
    - -std=*
    - -nostdinc

Diagnostics:
  Suppress:
    - unused-parameter
    - unused-variable
    - implicit-function-declaration
  ClangTidy:
    Remove:
      - readability-*
      - modernize-*
      - google-*
      - llvm-*

Index:
  Background: Build